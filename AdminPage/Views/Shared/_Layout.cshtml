    <!--CSS for Data table-->
    <style>
        .dataTables_length,
        .dataTables_filter {
            padding: 10px 0px;
            font-size: 20px;
            text-align: center;
        }

            .dataTables_filter label input {
                margin-left: 12px;
                border-radius: 30px;
                text-indent: 8px;
            }

        .dataTables_info {
            text-align: center;
            padding: 10px 0;
            font-size: 20px;
        }

        .dataTables_paginate {
            text-align: center;
            padding: 10px 0;
            font-size: 20px;
        }

        .paginate_button {
            margin: 0px 4px;
        }

            .paginate_button:hover {
                cursor: pointer;
            }
