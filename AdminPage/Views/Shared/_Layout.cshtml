<body>
    <!-- header -->
    <div class="header">
        <div class="container">
            <div class="w3layouts_logo">
                <a asp-action="Index" asp-controller="Home">
                    <h3 style="font-size: 34px;" class="text-center">High<span>Movies</span></h3>
                </a>
            </div>
            <div class="w3_search">
                @using (Html.BeginForm("Search", "Home", FormMethod.Post))
                {

                    <input type="text" id="Search" name="Search" placeholder="Search Movie By Name" required />
                    <input type="submit" value="Go" />
                }
            </div>
            <div class="w3l_sign_in_register">
                <ul>
                    <li><i class="fa fa-phone" aria-hidden="true"></i> 0845914368</li>

                    @*<li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Logout">Logout</a>
                    </li>*@

                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Login">Login</a>
                    </li>
                </ul>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
    <!-- nav -->
    <div class="movies_nav">
        <div class="container">
            <nav class="navbar navbar-default">
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="navbar-collapse navbar-right collapse" id="bs-example-navbar-collapse-1">
                    <nav>
                        <ul class="nav navbar-nav" style="display: flex; justify-content: center; align-items: center;">
                            <li class="nav-item"><a asp-controller="Home" asp-action="Index">Home</a></li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Account" asp-action="Index">Account</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Staff" asp-action="Index">Staff</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="Movie" asp-action="Index">Movie</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link text-dark" asp-area="" asp-controller="FastFood" asp-action="Index">FastFood</a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </nav>
        </div>
    </div>
    <!--  nav -->

    <div class="container">
        <main role="main" class="pb-3">@RenderBody()</main>
    </div>

    <!-- footer -->
    <footer>
        <div class="footer">
            <div class="container">
                <div class="w3ls_footer_grid">
                    <div class="col-md-6 w3ls_footer_grid_left">
                        <div class="w3ls_footer_grid_left1">
                            <h2>Subscribe to us</h2>
                            <div class="w3ls_footer_grid_left1_pos">
                                <form action="#" method="post">
                                    <input type="email" name="email" placeholder="Your email..." required="">
                                    <input type="submit" value="Send">
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 w3ls_footer_grid_right">
                        <a href="index.html">
                            <h2 style="padding-right: 77px;font-size: 40px;width: 160px;">
                                High
                                <span>Movies</span>
                            </h2>
                        </a>
                    </div>
                    <div class="clearfix"> </div>
                </div>
                <div class="col-md-5 w3ls_footer_grid1_left">
                    <p>
                        &copy; 2022 High Movies. All rights reserved | Design by <a href="#">HighGroup</a>
                    </p>
                </div>
                <div class="col-md-7 w3ls_footer_grid1_right">
